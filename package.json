{
  "name": "uniwars-api",
  "version": "0.0.0",
  "private": true,
  "main": "build_babel/server.js",
  "mainScript": "build_babel/scripts/main.js",
  "debug": "DEBUG='game*,http*,socket:game'",
  "scripts": {
    "start": "yarn build && NODE_ENV='PROD' node $npm_package_main",
    "dev": "NODE_ENV='DEV' DEBUG='game*,http*,socket:game' nodemon $npm_package_main",
    "build": "yarn run remove-types && babel build_flow -d build_babel",
    "remove-types": "flow-remove-types src --out-dir build_flow",
    "run-script": "yarn build && NODE_ENV='DEV' node $npm_package_mainScript",
    "watch": "$npm_package_scripts_build -w",
    "test": "NODE_ENV='TEST' jest",
    "open-docs": "esdoc && open docs/index.html",
    "docs": "esdoc"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "ajv": "^6.5.5",
    "awesome-phonenumber": "^2.2.5",
    "body-parser": "~1.18.2",
    "chance": "^1.0.16",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "debug": "~2.6.9",
    "express": "~4.15.5",
    "express-validator": "^5.3.0",
    "glicko2": "^0.8.4",
    "jsonwebtoken": "^8.4.0",
    "lodash": "^4.17.10",
    "mongodb": "^3.1.9",
    "monk": "^6.0.5",
    "morgan": "~1.9.0",
    "passport": "^0.4.0",
    "passport-google-oauth": "^1.0.0",
    "serve-favicon": "~2.4.5",
    "socket.io": "^2.1.0",
    "socketio-auth": "^0.1.0",
    "util": "^0.10.3",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-flow": "^7.0.0",
    "axios": "^0.18.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "esdoc": "^1.1.0",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-flow-type-plugin": "^1.1.0",
    "esdoc-standard-plugin": "^1.0.0",
    "flow-bin": "^0.83.0",
    "flow-remove-types": "^1.2.3",
    "jest": "^23.6.0",
    "mongodb-memory-server": "^2.7.0",
    "nodemon": "^1.18.4",
    "regenerator-runtime": "^0.12.1",
    "supertest": "^3.3.0"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/__tests__/config/"
    ],
    "globalSetup": "<rootDir>/__tests__/config/setup.js",
    "globalTeardown": "<rootDir>/__tests__/config/teardown.js",
    "testEnvironment": "<rootDir>/__tests__/config/mongo-env.js"
  }
}
