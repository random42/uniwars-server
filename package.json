{
  "name": "uniwars-api",
  "version": "0.0.0",
  "private": true,
  "main": "src/server.js",
  "debug": "DEBUG='game*,http*,socekt:game'",
  "scripts": {
    "start": "node $npm_package_main",
    "dev": "$npm_package_debug nodemon $npm_package_main",
    "build": "./node_modules/.bin/tsc",
    "watch": "./node_modules/.bin/tsc --watch",
    "test": "./node_modules/.bin/jest",
    "docs": "./node_modules/.bin/esdoc && open docs/index.html"
  },
  "dependencies": {
    "awesome-phonenumber": "^2.2.5",
    "bcrypt": "^3.0.2",
    "body-parser": "~1.18.2",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "debug": "~2.6.9",
    "express": "~4.15.5",
    "glicko2": "^0.8.4",
    "lodash": "^4.17.10",
    "mongodb": "^3.0.0-rc0",
    "monk": "^6.0.5",
    "morgan": "~1.9.0",
    "serve-favicon": "~2.4.5",
    "sharp": "0.20.1",
    "socket.io": "^2.1.0",
    "socketio-auth": "^0.1.0",
    "util": "^0.10.3"
  },
  "devDependencies": {
    "axios": "^0.18.0",
    "esdoc": "^1.1.0",
    "esdoc-standard-plugin": "^1.0.0",
    "jest": "^23.6.0",
    "nodemon": "^1.18.4",
    "supertest": "^3.3.0"
  },
  "esdoc": {
    "source": "src",
    "destination": "docs",
    "plugins": [
      {
        "name": "esdoc-standard-plugin"
      }
    ]
  }
}
